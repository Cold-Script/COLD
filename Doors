Tabs.Main:AddSection({"Notify"})
local entitynames = {"DreadMoving","BackdoorLookman","BackdoorRush","GloomPile","Eyes","JeffTheKiller","RushMoving","AmbushMoving","Snare","A60","A120"}
Tabs.Main:AddToggle({
  Name = "Notify Event",
  Description = "Enabled on <font color='rgb(255, 0, 0)'>Notification Event</font>",
  Default = false,
  Callback = function(v)
_G.Notify = v
if v then
addconnect = workspace.ChildAdded:Connect(function(v)
if table.find(entitynames,v.Name) then
repeat task.wait() until game.Players.LocalPlayer:DistanceFromCharacter(v:GetPivot().Position) < 1000 or not v:IsDescendantOf(workspace)
if v:IsDescendantOf(workspace) then
message(v.Name:gsub("Moving",""):lower().." has spawn")
end
end
end)
repeat task.wait() until not _G.Notify
addconnect:Disconnect()
end
end})
